{
  "name": "website",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "description": "Documentation website for Masonry Grid library.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrigenSoftware/masonry-grid.git",
    "directory": "website"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/masonry-grid/issues"
  },
  "scripts": {
    "clear": "del ./.astro ./dist",
    "dev": "astro dev",
    "copy-examples:regular": "cp ../examples/vanilla-regular/index.html ./public/vanilla-examples-frames/vanilla-regular.html",
    "copy-examples:balanced": "cp ../examples/vanilla-balanced/index.html ./public/vanilla-examples-frames/vanilla-balanced.html",
    "copy-examples:media-query": "cp ../examples/vanilla-media-query/index.html ./public/vanilla-examples-frames/vanilla-media-query.html",
    "copy-examples:infinite-scroll": "cp ../examples/vanilla-infinite-scroll/index.html ./public/vanilla-examples-frames/vanilla-infinite-scroll.html",
    "copy-examples": "run [ rm -rf ./public/vanilla-examples-frames/ ] [ mkdir -p ./public/vanilla-examples-frames/ ] copy-examples:regular copy-examples:balanced copy-examples:media-query copy-examples:infinite-scroll",
    "build": "run copy-examples [ astro check ] [ astro build ]",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint --flag v10_config_lookup_from_file"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/starlight": "^0.36.0",
    "astro": "^5.0.0",
    "astro-vtbot": "^2.1.9",
    "sharp": "^0.34.0",
    "starlight-llms-txt": "^0.6.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
