{
  "name": "website",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "description": "Documentation website for Masonry Grid library.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrigenSoftware/masonry-grid.git",
    "directory": "website"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/masonry-grid/issues"
  },
  "scripts": {
    "clear": "del ./.astro ./dist",
