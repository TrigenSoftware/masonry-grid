    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint --flag v10_config_lookup_from_file"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/starlight": "^0.36.0",
    "astro": "^5.0.0",
    "astro-vtbot": "^2.1.9",
    "sharp": "^0.34.0",
    "starlight-llms-txt": "^0.6.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
